  },
  "homepage": "http://shujianbu.github.io/DJShowcases",
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-loader": "^5.3.2",
    "chai": "^3.5.0",
    "commitizen": "^2.6.2",
    "cz-conventional-changelog": "^1.1.5",
    "eslint-plugin-react": "^3.16.1",
    "fast-csv": "^1.0.0",
    "google-spreadsheet": "^1.1.4",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
